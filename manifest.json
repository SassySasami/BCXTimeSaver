{
    "manifest_version": 3,
    "name": "MonModBC",
    "version": "0.1.0",
    "description": "Mod BC via bcModSDK",
    "host_permissions": [
      "https://www.bondageprojects.com/*",
      "https://bondage-europe.com/*"
    ],
    "content_scripts": [
      {
        "matches": [
          "https://www.bondageprojects.com/*",
          "https://bondageprojects.com/*",
          "https://www.bondage-europe.com/*"
        ],
        "js": ["injector.js"],
        "run_at": "document_start"
      }
    ],
    "web_accessible_resources": [
      {
        "resources": ["dist/mon-mod-bc.iife.js"],
        "matches": [
          "https://www.bondageprojects.com/*",
          "https://bondageprojects.com/*",
          "https://www.bondage-europe.com/*"
        ]
      }
    ]
  }
  